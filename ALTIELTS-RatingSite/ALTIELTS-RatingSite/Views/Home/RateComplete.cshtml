
@{
    ViewData["Title"] = "RateComplete";
}
<div class="row d-flex justify-content-center">
    <div class="card col-8 justify-content-center bg-light" style="outline:1px solid white; padding: 10px">
        <div>
            <p class="text-md-center font-weight-bold">Cảm ơn bạn đã tham gia đánh giá!</p>
        </div>
        <div style="margin: 5px">
            <img src="~/images/emoji/Asset.png" height="100" style="display:block; margin-left: auto; margin-right: auto" />
        </div>
        <button asp-action="Rate" class="btn btn-danger rounded-pill" style="margin-left: auto; margin-right: auto">
            <div class="text-center font-weight-bold">Đánh giá lại</div>
        </button>
    </div>
</div>

